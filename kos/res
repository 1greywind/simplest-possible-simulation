CLEARSCREEN.

DECLARE PARAMETER MODE IS "ALL".

IF MODE = "ALL" {
	PRINT "THESE ARE ALL THE RESOURCES ON THE SHIP:".
	LIST RESOURCES IN RESLIST.
	FOR RES IN RESLIST {
		PRINT "Resource " + RES:NAME + ": " + ROUND(RES:AMOUNT*RES:DENSITY*1000).
	}.
}.

PRINT "MASS: " + ROUND(SHIP:MASS*1000).